import java.util.Scanner;
public class Ha{
  public static void main(String[] args){
    System.out.print("请输入a和b两个数字：");
    Scanner in = new Scanner(System.in);
    int a = in.nextInt();
    int b = in.nextInt();
    a = a^b;
    b = a^b;
    a = a^b;
    System.out.println("a="+a+",b="+b);   //这是一种不需要构造第三变量便可交换变量的方法之一。
    }
 }
